{
	"name": "vitr",
	"private": true,
	"version": "0.0.0",
	"author": "Muhammad Yusuf <yusuf@iamyuu.dev> (https://iamyuu.dev)",
	"scripts": {
		"# general commands": "--------------------------------------------------",
		"cz": "git-cz",
		"setup": "npm install && husky install",
		"# compiler commands": "--------------------------------------------------",
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"# test commands": "--------------------------------------------------",
		"test": "is-ci-cli test:coverage test:watch",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"# linter commands": "--------------------------------------------------",
		"lint": "eslint --ignore-path .gitignore \"./src/**/*.{js,jsx,ts,tsx}\"",
		"format": "prettier --ignore-path .gitignore --write \"./src/**/*.{js,jsx,ts,tsx,css}\"",
		"#": "--------------------------------------------------"
	},
	"dependencies": {
		"react": "18.1.0",
		"react-dom": "18.1.0",
		"react-error-boundary": "3.1.4",
		"react-router-dom": "6.3.0"
	},
	"devDependencies": {
		"@babel/eslint-parser": "7.17",
		"@babel/preset-react": "7.17.12",
		"@commitlint/cli": "17.0",
		"@commitlint/config-conventional": "17.0",
		"@commitlint/cz-commitlint": "17.0",
		"@iconify-json/carbon": "1.1.5",
		"@testing-library/dom": "8.13.0",
		"@testing-library/jest-dom": "5.16.4",
		"@testing-library/react": "13.2.0",
		"@testing-library/user-event": "14.2.0",
		"@unocss/preset-icons": "0.34.0",
		"@unocss/preset-web-fonts": "0.34.0",
		"@unocss/reset": "0.34.0",
		"@vitejs/plugin-react": "1.3.2",
		"c8": "7.11.3",
		"commitizen": "4.2.4",
		"eslint": "8.15",
		"eslint-config-kentcdodds": "20.2",
		"eslint-config-prettier": "8.5",
		"eslint-import-resolver-alias": "1.1",
		"eslint-plugin-import": "2.26",
		"eslint-plugin-jest": "26.2",
		"eslint-plugin-jsdoc": "39.3",
		"eslint-plugin-jsx-a11y": "6.5",
		"eslint-plugin-prettier": "4.0",
		"eslint-plugin-react": "7.30",
		"happy-dom": "4.0.0",
		"husky": "8.0",
		"is-ci-cli": "2.2.0",
		"lint-staged": "12.4",
		"msw": "0.40.1",
		"prettier": "2.6",
		"pretty-quick": "3.1.3",
		"unocss": "0.34.0",
		"vite": "2.9.9",
		"vite-plugin-pages": "0.23.0",
		"vitest": "0.12.7"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/@commitlint/cz-commitlint"
		}
	}
}
