{
	"name": "vitr",
	"private": true,
	"version": "0.0.0",
	"author": "Muhammad Yusuf <yusuf@iamyuu.dev> (https://iamyuu.dev)",
	"scripts": {
		"# general commands": "--------------------------------------------------",
		"cz": "git-cz",
		"setup": "npm install && husky install",
		"# compiler commands": "--------------------------------------------------",
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"# test commands": "--------------------------------------------------",
		"test": "is-ci-cli test:coverage test:watch",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"# linter commands": "--------------------------------------------------",
		"lint": "eslint \"./src/**/*.{js,jsx}\"",
		"format": "prettier --write \"./src/**/*.{js,jsx,css}\"",
		"#": "--------------------------------------------------"
	},
	"dependencies": {
		"react": "18.1.0",
		"react-dom": "18.1.0",
		"react-error-boundary": "3.1.4",
		"react-router-dom": "6.3.0"
	},
	"devDependencies": {
		"@babel/eslint-parser": "7.17",
		"@babel/preset-react": "7.16.7",
		"@commitlint/cli": "16.2",
		"@commitlint/config-conventional": "16.2",
		"@commitlint/cz-commitlint": "16.2",
		"@iconify-json/carbon": "1.1.3",
		"@testing-library/dom": "8.13.0",
		"@testing-library/jest-dom": "5.16.4",
		"@testing-library/react": "13.1.1",
		"@testing-library/user-event": "14.1.1",
		"@unocss/preset-icons": "0.32.1",
		"@unocss/preset-web-fonts": "0.32.1",
		"@unocss/reset": "0.32.1",
		"@vitejs/plugin-react": "1.3.1",
		"c8": "7.11.2",
		"commitizen": "4.2.4",
		"eslint": "8.14",
		"eslint-config-kentcdodds": "20.2",
		"eslint-config-prettier": "8.5",
		"eslint-import-resolver-alias": "1.1",
		"eslint-plugin-import": "2.26",
		"eslint-plugin-jest": "26.1",
		"eslint-plugin-jsdoc": "39.2",
		"eslint-plugin-jsx-a11y": "6.5",
		"eslint-plugin-prettier": "4.0",
		"eslint-plugin-react": "7.29",
		"happy-dom": "3.1.0",
		"husky": "7.0",
		"is-ci-cli": "2.2.0",
		"jest-preview": "0.1.6",
		"lint-staged": "12.4",
		"msw": "0.39.2",
		"prettier": "2.6",
		"unocss": "0.32.1",
		"vite": "2.9.6",
		"vite-plugin-pages": "0.23.0",
		"vitest": "0.10.0"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/@commitlint/cz-commitlint"
		}
	}
}
